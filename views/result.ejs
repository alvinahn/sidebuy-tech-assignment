<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>Sidebuy Tech Assignment</title>
  <link rel="stylesheet" type="text/css" href="css/style.css"/>

</head>
<body>
  <table class="result-table">
    <tr>
      <th>Picture</th>
      <th>Name</th>
      <th>Score</th>
    </tr>
    <% for( var i = 0; i < data.length; i++) { %>
      <tr>
        <% if (JSON.stringify(data[i].photo_url) != "") { %>
          <th><img src=<%= JSON.stringify(data[i].photo_url).replace(/\"/g, "") %> alt=""></th>
        <% } %>
        <th><%= JSON.stringify(data[i].name).replace(/\"/g, "") %></th>
        <th><%= JSON.stringify(data[i].score) %></th>
      </tr>
    <% } %>
  </table>

  <a href="/" class="btn btn-search">Back to Search</a>
</body>
</html>